{
  "window.nativeTabs": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.fontFamily": "Hack Nerd Font Mono, Fira Code, Victor Mono, Iosevka Term, Cascadia Mono PL, UbuntuMono Nerd Font, Cascadia Code PL, VictorMono-Light, Monaco, JetBrains Mono NL, Monoid-Retina, Iosevka, SF Mono",
  "git.path": "/usr/bin/git",
  "editor.rulers": [110],
  "editor.wordWrap": "on",
  "editor.fontSize": 16,
  "editor.suggestSelection": "first",
  "editor.formatOnSaveMode": "modifications",
  "editor.lineNumbers": "relative",
  "editor.quickSuggestionsDelay": 0,
  "editor.formatOnPaste": true,
  "editor.snippetSuggestions": "top",
  "editor.inlineSuggest.enabled": true,
  // "editor.lineHeight": 26,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.renderLineHighlight": "none",
  "editor.guides.indentation": false,
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "window.title": "${dirty}${rootName}${separator}${activeEditorShort}",
  "workbench.tree.renderIndentGuides": "none",
  "quarto.mathjax.theme": "dark",
  "quarto.render.renderOnSave": true,
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.latex.autoClean.run": "onSucceeded",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.clean.method": "glob",
  "latex-workshop.latex.recipe.default": "first",
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk",
      "tools": ["latexmk"]
    },
    {
      "name": "pdflatex ➞ bibtex ➞ pdflatex`×2",
      "tools": ["pdflatex", "bibtex", "pdflatex", "pdflatex"]
    },
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    },
    {
      "name": "xelatex ➞ bibtex ➞ xelatex`×2",
      "tools": ["xelatex", "bibtex", "xelatex", "xelatex"]
    },
    {
      "name": "texify",
      "tools": ["texify"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"],
      "env": {}
    },
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "texify",
      "command": "texify",
      "args": [
        "--synctex",
        "--pdf",
        "--tex-option=\"-interaction=nonstopmode\"",
        "--tex-option=\"-file-line-error\"",
        "%DOC%.tex"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.bcf",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.run.xml",
    "*.synctex.gz",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz(busy)",
    "*.nav",
    "*.vrb"
  ],
  "workbench.colorTheme": "Monokai",
  "workbench.sideBar.location": "right",
  "workbench.panel.defaultLocation": "left",
  "explorer.autoReveal": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "r.rterm.mac": "/Users/utkuturk/opt/miniconda3/bin/radian",
  "r.bracketedPaste": true,
  "r.plot.useHttpgd": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.inheritEnv": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.scrollback": 10000,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.exclude": {
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false,
    "**/CVS": false
  },
  "search.exclude": {
    "**/.git": true
  },
  "telemetry.telemetryLevel": "off",
  "github.copilot.enable": {
    "quarto": false,
    "*": true
  },
  "gitlens.hovers.currentLine.over": "line",
  "git.confirmSync": false,
  "quarto.path": "/usr/local/bin/quarto",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[quarto]": {
    "editor.snippetSuggestions": "inline"
  },
  "symbols.hidesExplorerArrows": false,
  "workbench.iconTheme": "a-file-icon-vscode",
  "tabnine.experimentalAutoImports": true,
  "github.copilot.advanced": {},
  "[dockercompose]": {
    "editor.tabSize": 4
  },
  "terminal.integrated.tabs.enabled": false,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.fontLigatures": true,
  "workbench.editor.showTabs": "single",
  "[latex]": {
    "editor.defaultFormatter": "nickfode.latex-formatter"
  },
  "window.titleBarStyle": "native",
  "apc.electron": {
    "titleBarStyle": "customButtonsOnHover"
  },
  "apc.header": {
    "height": 37
  },
  "apc.sidebar.titlebar": {
    "height": 37
  },
  "apc.activityBar": {
    "size": 77,
    "itemSize": 48,
    "itemMargin": 0
  },
  "apc.stylesheet": {
    ".custom-sidebar-titlebar .sidebar .composite.title": "padding-left: 0;", // Don't indent the sidebar title. We want it horizontally aligned to the left so that it makes a nice vertical column with the controls in the sidebar.
    ".custom-sidebar-titlebar .sidebar .composite.title .inline-titlebar-placeholder": "padding-left: 0 !important;", // Don't indent the sidebar title. Part 2 to remove the empty space to the left of the sidebar title and complete the proper horizontal left alignment.
    ".monaco-workbench .part.statusbar>.items-container>.statusbar-item.left.first-visible-item": "padding-left: 0;" // Don't indent the statusbar items. This horizontally aligns the statusbar items with the "Problems" header in the panel, which looks nice.
  }
}
